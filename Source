function h()
workspace.Remote.ItemHandler:InvokeServer({Position=game.Players.LocalPlayer.Character.Head.Position,Parent=workspace.Prison_ITEMS:FindFirstChild("AK-47",true)})
workspace.Remote.ItemHandler:InvokeServer({Position=game.Players.LocalPlayer.Character.Head.Position,Parent=workspace.Prison_ITEMS:FindFirstChild("Remington 870",true)})
workspace.Remote.ItemHandler:InvokeServer({Position=game.Players.LocalPlayer.Character.Head.Position,Parent=workspace.Prison_ITEMS:FindFirstChild("M9",true)})
workspace.Remote.ItemHandler:InvokeServer({Position=game.Players.LocalPlayer.Character.Head.Position,Parent=workspace.Prison_ITEMS:FindFirstChild("M4A1",true)})
end
local b=game:GetService("Players").LocalPlayer
FindPlayer=function(i,j)
if string.lower(i)=="me" then
return b
else
i=i:gsub("%s+","")
for k,l in pairs(game:GetService("Players"):GetPlayers()) do
if l.Name:lower():match("^"..i:lower()) or l.DisplayName:lower():match("^"..i:lower()) then
return l
end
end
end
return nil
end
function BringPlayer(m,n)
local o=game:GetService("Players").LocalPlayer.TeamColor.Name
local p=game:GetService("Players").LocalPlayer.Character:GetPrimaryPartCFrame()
if not n then
n=p
end
workspace.Remote.TeamEvent:FireServer("Bright blue")
wait(.2)
game:GetService("Players").LocalPlayer.Character:SetPrimaryPartCFrame(p)
wait()
local q=m
local r=game:GetService("Players").LocalPlayer.Character
local s=game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Handcuffs")
r:FindFirstChildOfClass("Humanoid"):Destroy()
Instance.new("Humanoid",r)
workspace.CurrentCamera.CameraSubject=r
s.Parent=r
repeat task.wait()
r:SetPrimaryPartCFrame(n)
q.Character:SetPrimaryPartCFrame(r:GetPrimaryPartCFrame())
if firetouchinterest then
pcall(function()
firetouchinterest(q.Character.Head,s.Handle,0)
firetouchinterest(q.Character.Head,s.Handle,1)
end)
end
until s.Parent~=r or not r or not s or not q.Character or q.Character:FindFirstChild("Handcuffs")
local t=nil
wait(.25)
t=game:GetService("Players").LocalPlayer.CharacterAdded:Connect(function(u)
repeat task.wait() until u and u:FindFirstChildOfClass("Humanoid")
for v=1,2 do
u:SetPrimaryPartCFrame(p)
task.wait()
end
if o=="Really red" then
wait(.3)
local w=workspace["Criminals Spawn"].SpawnLocation
local x=w.CFrame
repeat task.wait()w.CFrame=game:GetService("Players").LocalPlayer.Character:GetPrimaryPartCFrame() until game:GetService("Players").LocalPlayer.Team==game.Teams.Criminals
wait()
w.CFrame=x
end
t:Disconnect()
end)
if o~="Really red" then
workspace.Remote.TeamEvent:FireServer(o)
end
end
task.spawn(function()
local y=Instance.new("ScreenGui")
y.Name="WelcomePopup"
y.ResetOnSpawn=false
local z=Instance.new("TextLabel")
z.Name="Message"
z.Size=UDim2.new(0,300,0,50)
z.Position=UDim2.new(0.5,-150,0.5,-25)
z.BackgroundColor3=Color3.fromRGB(0,0,0)
z.BackgroundTransparency=1
z.TextColor3=Color3.fromRGB(255,255,255)
z.TextTransparency=1
z.TextScaled=true
z.Font=Enum.Font.SourceSansBold
z.Text="Welcome to starline admin made by egodestroyer!"
z.Parent=y
y.Parent=game.Players.LocalPlayer.PlayerGui
local A=TweenInfo.new(1,Enum.EasingStyle.Linear,Enum.EasingDirection.In)
local B=game:GetService("TweenService"):Create(z,A,{BackgroundTransparency=1,TextTransparency=0})
B:Play()
wait(2)
local C=TweenInfo.new(1,Enum.EasingStyle.Linear,Enum.EasingDirection.Out)
local D=game:GetService("TweenService"):Create(z,C,{Position=UDim2.new(0.5,-150,0,-50),BackgroundTransparency=1,TextTransparency=1})
D:Play()
D.Completed:Connect(function()
y:Destroy()
end)
wait(2)
local y=Instance.new("ScreenGui")
y.Name="WelcomePopup"
y.ResetOnSpawn=false
local z=Instance.new("TextLabel")
z.Name="Message"
z.Size=UDim2.new(0,300,0,50)
z.Position=UDim2.new(0.5,-150,0.5,-25)
z.BackgroundColor3=Color3.fromRGB(0,0,0)
z.BackgroundTransparency=1
z.TextColor3=Color3.fromRGB(255,255,255)
z.TextTransparency=1
z.TextScaled=true
z.Font=Enum.Font.SourceSansBold
z.Text="by egodestroyerontop!"
z.Parent=y
y.Parent=game.Players.LocalPlayer.PlayerGui
local A=TweenInfo.new(1,Enum.EasingStyle.Linear,Enum.EasingDirection.In)
local B=game:GetService("TweenService"):Create(z,A,{BackgroundTransparency=1,TextTransparency=0})
B:Play()
wait(2)
local C=TweenInfo.new(1,Enum.EasingStyle.Linear,Enum.EasingDirection.Out)
local D=game:GetService("TweenService"):Create(z,C,{Position=UDim2.new(0.5,-150,0,-50),BackgroundTransparency=1,TextTransparency=1})
D:Play()
D.Completed:Connect(function()
y:Destroy()
end)
end)
b.Chatted:Connect(function(E)
local F=E:split(" ")
if F[1]=="?kill" then
local G=FindPlayer(F[2])
if G and G.Character and G.Character:FindFirstChild("HumanoidRootPart") then
for H=1,15 do
game:GetService("ReplicatedStorage").meleeEvent:FireServer(G)
end
end
elseif F[1]=="?bring" then
local G=FindPlayer(F[2])
if G and G.Character and G.Character:FindFirstChild("HumanoidRootPart") then
BringPlayer(G)
end
elseif F[1]=="?killall" then
for I,J in pairs(game.Players:GetPlayers()) do
if J.Character and J.Character:FindFirstChild("HumanoidRootPart") then
for H=1,15 do
game:GetService("ReplicatedStorage").meleeEvent:FireServer(J)
end
end
end
elseif F[1]=="?kcops" then
for I,J in pairs(game.Players:GetPlayers()) do
if J.TeamColor.Name=="Bright blue"then
if J.Character and J.Character:FindFirstChild("HumanoidRootPart") then
for H=1,15 do
game:GetService("ReplicatedStorage").meleeEvent:FireServer(J)
end
end
end
end
elseif F[1]=="?kinmate" then
for I,J in pairs(game.Players:GetPlayers()) do
if J.TeamColor.Name=="Bright orange"then
if J.Character and J.Character:FindFirstChild("HumanoidRootPart") then
for H=1,15 do
game:GetService("ReplicatedStorage").meleeEvent:FireServer(J)
end
end
end
end
elseif F[1]=="?kcrim" then
for I,J in pairs(game.Players:GetPlayers()) do
if J.TeamColor.Name=="Really red"then
if J.Character and J.Character:FindFirstChild("HumanoidRootPart") then
for H=1,15 do
game:GetService("ReplicatedStorage").meleeEvent:FireServer(J)
end
end
end
end
elseif F[1]=="?arrest" then
local G=FindPlayer(F[2])
if G and G.Character and G.Character:FindFirstChild("HumanoidRootPart")then
local K=b.Backpack:FindFirstChild("Handcuffs")or b.Character:FindFirstChild("Handcuffs")
K.Parent=b.Character
fireclickdetector(workspace["Prison_Halls"].cuff["Click"].ClickDetector,0)
fireclickdetector(workspace["Prison_Halls"].cuff["Click"].ClickDetector,1)
for H=1,15 do
game:GetService("ReplicatedStorage").meleeEvent:FireServer(G)
end
end
elseif F[1]=="?arrestall" then
local K=b.Backpack:FindFirstChild("Handcuffs")or b.Character:FindFirstChild("Handcuffs")
K.Parent=b.Character
fireclickdetector(workspace["Prison_Halls"].cuff["Click"].ClickDetector,0)
fireclickdetector(workspace["Prison_Halls"].cuff["Click"].ClickDetector,1)
for I,J in pairs(game.Players:GetPlayers()) do
if J.Character and J.Character:FindFirstChild("HumanoidRootPart") then
for H=1,15 do
game:GetService("ReplicatedStorage").meleeEvent:FireServer(J)
end
end
end
elseif F[1]=="?carrest" then
for I,J in pairs(game.Players:GetPlayers())do
if J.TeamColor.Name=="Bright blue"then
workspace.Remote.TeamEvent:FireServer("Bright orange")
task.wait(.5)
workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver:FindFirstChild("Remington 870").ITEMPICKUP)
workspace.Remote.TeamEvent:FireServer("Bright blue")
task.wait(.5)
if J.Character and J.Character:FindFirstChild("HumanoidRootPart")then
local K=b.Backpack:FindFirstChild("Handcuffs")or b.Character:FindFirstChild("Handcuffs")
K.Parent=b.Character
fireclickdetector(workspace["Prison_Halls"].cuff["Click"].ClickDetector,0)
fireclickdetector(workspace["Prison_Halls"].cuff["Click"].ClickDetector,1)
for H=1,15 do
game:GetService("ReplicatedStorage").meleeEvent:FireServer(J)
end
end
end
end
elseif F[1]=="?lk"then
local G=FindPlayer(F[2])
if G and G.Character and G.Character:FindFirstChild("HumanoidRootPart")then
getgenv().loopkill=true
repeat
if not getgenv().loopkill then break end
game:GetService("ReplicatedStorage").meleeEvent:FireServer(G)
task.wait()
until not getgenv().loopkill or not G.Character or not G.Character:FindFirstChild("HumanoidRootPart")
end
elseif F[1]=="?stoplk"then
getgenv().loopkill=false
elseif F[1]=="?spin"then
getgenv().spin=true
while task.wait()do
if getgenv().spin then
b.Character:SetPrimaryPartCFrame(b.Character:GetPrimaryPartCFrame()*CFrame.Angles(0,math.rad(10),0))
else
break
end
end
elseif F[1]=="?unspin"then
getgenv().spin=false
elseif F[1]=="?infjump"then
getgenv().infjump=true
game:GetService("UserInputService").JumpRequest:Connect(function()
if getgenv().infjump then
b.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping")
end
end)
elseif F[1]=="?mcrim"then
local G=FindPlayer(F[2])
if G and G.Character and G.Character:FindFirstChild("HumanoidRootPart")then
b.Character:SetPrimaryPartCFrame(CFrame.new(915.821,99.989,2338.7))
repeat task.wait()
G.Character:SetPrimaryPartCFrame(b.Character:GetPrimaryPartCFrame())
until G.TeamColor.Name=="Really red"or not G or not G.Character or not G.Character:FindFirstChild("HumanoidRootPart")
end
elseif F[1]=="?to"then
local G=FindPlayer(F[2])
if G and G.Character and G.Character:FindFirstChild("HumanoidRootPart")then
b.Character:SetPrimaryPartCFrame(G.Character:GetPrimaryPartCFrame())
end
elseif F[1]=="?spunch"then
local L=Instance.new("HopperBin")
L.Parent=b.Backpack
L.BinType="Hammer"
local M
M=hookmetamethod(game,"__namecall",function(self,...)
local N={...}
local O=getnamecallmethod()
if not checkcaller()and self==b and O=="Kick"then
return
elseif not checkcaller()and tostring(self)=="meleeEvent"and O=="FireServer"and N[2]==b.Backpack:FindFirstChild("HopperBin")then
N[1]=b:GetMouse().Target
end
return M(self,unpack(N))
end)
elseif F[1]=="?whitelist"then
getgenv().whitelisted=F[2]
elseif F[1]=="?unwhitelist"then
getgenv().whitelisted=""
elseif F[1]=="?ndoor"then
for I,P in pairs(game.Workspace.Doors:GetDescendants())do
if P:IsA("BasePart")then
firetouchinterest(b.Character.Head,P,0)
end
end
elseif F[1]=="?rj"then
game:GetService("TeleportService"):Teleport(game.PlaceId,game:GetService("Players").LocalPlayer)
elseif F[1]=="?ad"then
local Q=Instance.new("Message",workspace)
Q.Text="STAY STRONGER WITH STARLINE ADMIN, https://discord.gg/NBjt9W5zeU"
wait(3)
Q:Destroy()
elseif F[1]=="?sword"then
workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver:FindFirstChild("M9").ITEMPICKUP)
wait(0.5)
b.Backpack:FindFirstChild("M9"):Destroy()
local R=Instance.new("Tool",b.Backpack)
R.RequiresHandle=false
R.Name="Sword"
R.Activated:Connect(function()
local S={}
for I,J in pairs(game.Players:GetPlayers())do
if J.TeamColor.Name~="Bright blue"and J.Character and J.Character:FindFirstChild("HumanoidRootPart")then
table.insert(S,J)
end
end
for I,J in pairs(S)do
game:GetService("ReplicatedStorage").meleeEvent:FireServer(J)
end
end)
elseif F[1]=="?guns"then
h()
elseif F[1]=="?gmod"then
h()
wait(1)
for I,P in pairs(b.Backpack:GetChildren())do
if P:IsA("Tool")then
P.Grip=CFrame.new(0,0,0)*CFrame.Angles(math.rad(-90),0,0)
local T=P:FindFirstChild("GunStates")or P:FindFirstChild("State")
for I,U in pairs(T:GetChildren())do
if U.Name=="StoredAmmo"or U.Name=="Ammo"then
U:Destroy()
end
end
Instance.new("BoolValue",T).Name="Automatic"
local V=Instance.new("IntValue",T)
V.Name="Damage"
V.Value=100
local W=Instance.new("NumberValue",T)
W.Name="FireRate"
W.Value=0.05
local X=Instance.new("NumberValue",T)
X.Name="MaxAmmo"
X.Value=1e6
local Y=Instance.new("NumberValue",T)
Y.Name="CurrentAmmo"
Y.Value=1e6
end
end
elseif F[1]=="?ak"then
workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver:FindFirstChild("AK-47").ITEMPICKUP)
elseif F[1]=="?m4"then
workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver:FindFirstChild("M4A1").ITEMPICKUP)
elseif F[1]=="?m9"then
workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver:FindFirstChild("M9").ITEMPICKUP)
elseif F[1]=="?shotgun"then
workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver:FindFirstChild("Remington 870").ITEMPICKUP)
elseif F[1]=="?lgun"then
local Z=Instance.new("Tool",b.Backpack)
Z.Name="LagGun"
Z.RequiresHandle=false
Z.Activated:Connect(function()
while task.wait()do
if not Z:IsDescendantOf(b)then
break
end
for I,J in pairs(game.Players:GetPlayers())do
game:GetService("ReplicatedStorage").meleeEvent:FireServer(J)
end
end
end)
elseif F[1]=="?cop"then
workspace.Remote.TeamEvent:FireServer("Bright blue")
elseif F[1]=="?inmate"then
workspace.Remote.TeamEvent:FireServer("Bright orange")
elseif F[1]=="?crim"then
b.Character.HumanoidRootPart.CFrame=CFrame.new(-919.958,95.327,2138.189)
end
end)
